{
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        },
    },
    "plugins": [
        "react"
    ]
}
